{
  "name": "gw2w2w-cloudflare",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo dev --parallel",
    "preinstall": "npx only-allow pnpm",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "devtools": "turbo devtools",
    "check-types": "turbo run check-types",
    "check-deps": "pnpm knip --max-show-issues 5",
    "clean:wrangler": "pnpm -r exec npx rimraf .wrangler/state"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "knip": "^5.81.0",
    "prettier": "^3.7.4",
    "turbo": "^2.7.5",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.15.4",
  "dependencies": {
    "knix": "^0.0.4"
  }
}
